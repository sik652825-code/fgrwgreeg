async def get_entity_safe(entity):
    """–ë–µ–∑–æ–ø–∞—Å–Ω–æ–µ –ø–æ–ª—É—á–µ–Ω–∏–µ —Å—É—â–Ω–æ—Å—Ç–∏"""
    try:
        if isinstance(entity, int):
            return await bot.get_entity(entity)
        elif isinstance(entity, str):
            if entity.isdigit():
                return await bot.get_entity(int(entity))
            elif entity.startswith('@'):
                return await bot.get_entity(entity)
            else:
                try:
                    return await bot.get_entity(int(entity))
                except:
                    return await bot.get_entity(entity)
        elif isinstance(entity, PeerUser):
            return await bot.get_entity(entity.user_id)
        else:
            return await bot.get_entity(entity)
    except Exception as e:
        logger.error(f"–û—à–∏–±–∫–∞ –ø–æ–ª—É—á–µ–Ω–∏—è —Å—É—â–Ω–æ—Å—Ç–∏: {e}")
        raise

async def send_response(event, text, buttons=None, parse_mode='md', link_preview=False):
    """–£–Ω–∏–≤–µ—Ä—Å–∞–ª—å–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –æ—Ç–ø—Ä–∞–≤–∫–∏ –æ—Ç–≤–µ—Ç–∞"""
    try:
        if buttons:
            return await event.respond(text, buttons=buttons, parse_mode=parse_mode, link_preview=link_preview)
        else:
            return await event.respond(text, parse_mode=parse_mode, link_preview=link_preview)
    except Exception as e:
        logger.error(f"–û—à–∏–±–∫–∞ –æ—Ç–ø—Ä–∞–≤–∫–∏ —Å–æ–æ–±—â–µ–Ω–∏—è: {e}")
        return None

