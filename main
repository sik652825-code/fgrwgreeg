    def update_warnings(self, user_id, admin_id=None):
        """–£–≤–µ–ª–∏—á–∏–≤–∞–µ—Ç –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏–π"""
        try:
            self.cursor.execute(
                'UPDATE users SET warnings = warnings + 1 WHERE user_id = ?',
                (user_id,)
            )
            self.conn.commit()
            
            # –õ–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ –≤—ã–¥–∞—á–∏ –ø—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏—è
            if admin_id:
                warnings_count = self.get_warnings_count(user_id)
                self.add_log(
                    action_type="WARNING_GIVEN",
                    admin_id=admin_id,
                    target_id=user_id,
                    details=f"–í—ã–¥–∞–Ω–æ –ø—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏–µ (–≤—Å–µ–≥–æ: {warnings_count})"
                )
        except Exception as e:
            logger.error(f"–û—à–∏–±–∫–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –ø—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏–π: {e}")

    def get_warnings_count(self, user_id):
        """–ü–æ–ª—É—á–∞–µ—Ç –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏–π"""
        user = self.get_user(user_id)
        return user['warnings'] if user else 0

    def reset_warnings(self, user_id, admin_id=None):
        """–°–±—Ä–∞—Å—ã–≤–∞–µ—Ç –ø—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏—è"""
