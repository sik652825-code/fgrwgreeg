@bot.on(events.NewMessage(pattern=r'(?i)^/–ª–æ–≥–∏|/logs'))
async def show_logs_command(event):
    """–ü–æ–∫–∞–∑–∞—Ç—å –ø–æ—Å–ª–µ–¥–Ω–∏–µ –ª–æ–≥–∏"""
    user_id = event.sender_id
    
    # –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–∞–≤
    if not can_manage_roles(user_id, 0):
        return await send_response(event, "‚ùå –£ –≤–∞—Å –Ω–µ—Ç –ø—Ä–∞–≤ –¥–ª—è –ø—Ä–æ—Å–º–æ—Ç—Ä–∞ –ª–æ–≥–æ–≤")
    
    logs = db.get_recent_logs(20)
    
    if not logs:
        return await send_response(event, "üì≠ –õ–æ–≥–∏ –æ—Ç—Å—É—Ç—Å—Ç–≤—É—é—Ç")
    
    log_text = "üìã **–ü–æ—Å–ª–µ–¥–Ω–∏–µ 20 –ª–æ–≥–æ–≤:**\n\n"
    
    for log in logs:
        action_type = log['action_type']
        timestamp = log['created_at'][:19]
        
        # –§–æ—Ä–º–∞—Ç–∏—Ä—É–µ–º —Ç–∏–ø –¥–µ–π—Å—Ç–≤–∏—è
        action_names = {
            "ROLE_GRANT": "–í—ã–¥–∞—á–∞ —Ä–æ–ª–∏",
            "ROLE_REMOVED": "–°–Ω—è—Ç–∏–µ —Ä–æ–ª–∏",
            "TRUST_GRANTED": "–í—ã–¥–∞—á–∞ —Ç—Ä–∞—Å—Ç–∞",
            "TRUST_REMOVED": "–°–Ω—è—Ç–∏–µ —Ç—Ä–∞—Å—Ç–∞",
            "SCAMMER_ADDED": "–î–æ–±–∞–≤–ª–µ–Ω–∏–µ —Å–∫–∞–º–º–µ—Ä–∞",
            "SCAMMER_REMOVED": "–°–Ω—è—Ç–∏–µ —Å–∫–∞–º–º–µ—Ä–∞",
            "SCAMMER_MARKED": "–û—Ç–º–µ—Ç–∫–∞ —Å–∫–∞–º–º–µ—Ä–∞",
            "SCAMMER_SLEPT": "–°–∫–∞–º–µ—Ä —Å–ª–∏—Ç",
            "PREMIUM_GRANT": "–í—ã–¥–∞—á–∞ –ø—Ä–µ–º–∏—É–º–∞",
            "PREMIUM_REMOVED": "–£–¥–∞–ª–µ–Ω–∏–µ –ø—Ä–µ–º–∏—É–º–∞",
            "WARNING_GIVEN": "–í—ã–¥–∞—á–∞ –ø—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏—è",
            "WARNING_REMOVED": "–°–Ω—è—Ç–∏–µ –ø—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏—è",
            "WARNINGS_RESET": "–°–±—Ä–æ—Å –ø—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏–π",
            "THANKS_GIVEN": "–í—ã–¥–∞—á–∞ –±–ª–∞–≥–æ–¥–∞—Ä–Ω–æ—Å—Ç–∏"
        }
        
        action_name = action_names.get(action_type, action_type)
        
        log_text += f"**{action_name}**\n"
        log_text += f"üïê {timestamp}\n"
        
        if log['admin_id']:
            log_text += f"üëÆ Admin ID: `{log['admin_id']}`\n"
        if log['target_id']:
            log_text += f"üë§ Target ID: `{log['target_id']}`\n"
        if log['details']:
            log_text += f"üìù {log['details']}\n"
        
        log_text += "‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ\n"
    
    await send_response(event, log_text)

# ========== –ó–ê–ü–£–°–ö –ë–û–¢–ê ==========

def main():
    """–û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –∑–∞–ø—É—Å–∫–∞"""
    logger.info("=" * 50)
    logger.info("–ë–æ—Ç Playing ANTISCAM –∑–∞–ø—É—Å–∫–∞–µ—Ç—Å—è...")
    logger.info(f"–í–ª–∞–¥–µ–ª—å—Ü—ã: {OWNER_IDS}")
    logger.info(f"–ê–¥–º–∏–Ω—ã: {ADMINS}")
    logger.info(f"–ö–∞–Ω–∞–ª –ª–æ–≥–æ–≤: {LOG_CHANNEL}")
    logger.info(f"–°—Å—ã–ª–∫–∞: https://t.me/playingantiscam")
    logger.info("=" * 50)
    
    try:
        bot.start(bot_token=BOT_TOKEN)
        logger.info("–ë–æ—Ç —É—Å–ø–µ—à–Ω–æ –∑–∞–ø—É—â–µ–Ω!")
        
        async def notify_owners():
            for owner_id in OWNER_IDS:
                try:
                    await get_entity_safe(owner_id)
                    await bot.send_message(
                        owner_id,
                        "‚úÖ –ë–æ—Ç Playing ANTISCAM —É—Å–ø–µ—à–Ω–æ –∑–∞–ø—É—â–µ–Ω!\n"
                        f"–í—Ä–µ–º—è –∑–∞–ø—É—Å–∫–∞: {datetime.now().strftime('%Y-%m-%d %H:%M:%S')}\n"
                        f"–°—Å—ã–ª–∫–∞: https://t.me/playingantiscam\n"
                        f"–°–∏—Å—Ç–µ–º–∞ –ø—Ä–∞–≤ –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–∞! –í–ª–∞–¥–µ–ª—å—Ü—ã –∏–º–µ—é—Ç –ø–æ–ª–Ω—ã–π –¥–æ—Å—Ç—É–ø."
                    )
                except Exception as e:
                    logger.warning(f"–ù–µ —É–¥–∞–ª–æ—Å—å –æ—Ç–ø—Ä–∞–≤–∏—Ç—å —Å–æ–æ–±—â–µ–Ω–∏–µ –≤–ª–∞–¥–µ–ª—å—Ü—É {owner_id}: {e}")
        
        bot.loop.create_task(notify_owners())
        
        bot.run_until_disconnected()
        
    except KeyboardInterrupt:
        logger.info("–ë–æ—Ç –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–º")
    except Exception as e:
        logger.error(f"–ö—Ä–∏—Ç–∏—á–µ—Å–∫–∞—è –æ—à–∏–±–∫–∞: {e}")
    finally:
        db.close()
        logger.info("–ë–æ—Ç –∑–∞–≤–µ—Ä—à–∏–ª —Ä–∞–±–æ—Ç—É")

if __name__ == "__main__":
    main()
