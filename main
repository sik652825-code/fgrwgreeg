class Database:
    def __init__(self, db_path="playing_antiscam.db"):
        self.conn = sqlite3.connect(db_path, check_same_thread=False)
        self.conn.row_factory = sqlite3.Row
        self.cursor = self.conn.cursor()
        self.init_tables()
        logger.info("–ë–∞–∑–∞ –¥–∞–Ω–Ω—ã—Ö –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∏—Ä–æ–≤–∞–Ω–∞")

    def init_tables(self):
        """–ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è —Ç–∞–±–ª–∏—Ü —Å –º–∏–≥—Ä–∞—Ü–∏–µ–π"""
        # –¢–∞–±–ª–∏—Ü–∞ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π
        self.cursor.execute('''
            CREATE TABLE IF NOT EXISTS users (
                id INTEGER PRIMARY KEY AUTOINCREMENT,
                user_id INTEGER UNIQUE NOT NULL,
                username TEXT,
                role_id INTEGER DEFAULT 0,
                country TEXT DEFAULT '–ù–µ —É–∫–∞–∑–∞–Ω–∞',
                channel TEXT DEFAULT '‚ùì',
                description TEXT,
                premium_expiry DATETIME,
                scammers_slept INTEGER DEFAULT 0,
                check_count INTEGER DEFAULT 0,
                warnings INTEGER DEFAULT 0,
                custom_photo_url TEXT,
                granted_by_id INTEGER,
                premium_points INTEGER DEFAULT 1000,
                created_at DATETIME DEFAULT CURRENT_TIMESTAMP
            )
        ''')

        # –¢–∞–±–ª–∏—Ü–∞ —Å–∫–∞–º–º–µ—Ä–æ–≤
        self.cursor.execute('''
            CREATE TABLE IF NOT EXISTS scammers (
                id INTEGER PRIMARY KEY AUTOINCREMENT,
                user_id INTEGER NOT NULL,
                reason TEXT,
                added_by INTEGER,
                evidence TEXT,
                unique_id TEXT UNIQUE,
                added_at DATETIME DEFAULT CURRENT_TIMESTAMP,
                added_by_username TEXT,
                scammer_username TEXT,
                granted_by_id INTEGER
            )
        ''')

        # –¢–∞–±–ª–∏—Ü–∞ –¥–æ–≤–µ—Ä–∏—è
        self.cursor.execute('''
            CREATE TABLE IF NOT EXISTS trust (
                id INTEGER PRIMARY KEY AUTOINCREMENT,
                user_id INTEGER UNIQUE NOT NULL,
                granted_by INTEGER,
                grant_date DATETIME,
                FOREIGN KEY (user_id) REFERENCES users (user_id)
            )
        ''')

        # –¢–∞–±–ª–∏—Ü–∞ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
        self.cursor.execute('''
            CREATE TABLE IF NOT EXISTS statistics (
                id INTEGER PRIMARY KEY DEFAULT 1,
                total_messages INTEGER DEFAULT 0,
                last_update DATETIME
            )
        ''')

        # –¢–∞–±–ª–∏—Ü–∞ —Å–æ–æ–±—â–µ–Ω–∏–π –¥–ª—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
        self.cursor.execute('''
            CREATE TABLE IF NOT EXISTS messages (
                id INTEGER PRIMARY KEY AUTOINCREMENT,
                user_id INTEGER,
                timestamp DATETIME DEFAULT CURRENT_TIMESTAMP
            )
        ''')

        # –¢–∞–±–ª–∏—Ü–∞ –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã—Ö –ø—Ä–∏—á–∏–Ω –¥–ª—è —Å–∫–∞–º–º–µ—Ä–æ–≤
        self.cursor.execute('''
            CREATE TABLE IF NOT EXISTS additional_reasons (
                id INTEGER PRIMARY KEY AUTOINCREMENT,
                user_id INTEGER NOT NULL,
                additional_reason TEXT,
                added_at DATETIME DEFAULT CURRENT_TIMESTAMP
            )
        ''')

        # –¢–∞–±–ª–∏—Ü–∞ –ª–æ–≥–æ–≤
        self.cursor.execute('''
            CREATE TABLE IF NOT EXISTS logs (
                id INTEGER PRIMARY KEY AUTOINCREMENT,
                action_type TEXT NOT NULL,
                user_id INTEGER,
                admin_id INTEGER,
                target_id INTEGER,
                details TEXT,
                created_at DATETIME DEFAULT CURRENT_TIMESTAMP
            )
        ''')

        # –¢–∞–±–ª–∏—Ü–∞ –ø—Ä–µ–º–∏—É–º –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π
        self.cursor.execute('''
            CREATE TABLE IF NOT EXISTS premium_users (
                id INTEGER PRIMARY KEY AUTOINCREMENT,
                user_id INTEGER UNIQUE NOT NULL,
                premium_expiry DATETIME,
                custom_photo_url TEXT,
                custom_channel TEXT,
                granted_by INTEGER,
                granted_at DATETIME DEFAULT CURRENT_TIMESTAMP,
                FOREIGN KEY (user_id) REFERENCES users (user_id)
            )
        ''')
        
        # –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
        self.cursor.execute('INSERT OR IGNORE INTO statistics (id) VALUES (1)')
        self.conn.commit()

    # ========== –õ–û–ì–ò–†–û–í–ê–ù–ò–ï ==========
    
    def add_log(self, action_type, user_id=None, admin_id=None, target_id=None, details=""):
        """–î–æ–±–∞–≤–ª—è–µ—Ç –∑–∞–ø–∏—Å—å –≤ –ª–æ–≥"""
        try:
            self.cursor.execute('''
                INSERT INTO logs (action_type, user_id, admin_id, target_id, details)
                VALUES (?, ?, ?, ?, ?)
            ''', (action_type, user_id, admin_id, target_id, details))
            self.conn.commit()
            logger.info(f"–õ–æ–≥ –¥–æ–±–∞–≤–ª–µ–Ω: {action_type} - {details}")
        except Exception as e:
            logger.error(f"–û—à–∏–±–∫–∞ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –ª–æ–≥–∞: {e}")

    def get_recent_logs(self, limit=20):
        """–ü–æ–ª—É—á–∞–µ—Ç –ø–æ—Å–ª–µ–¥–Ω–∏–µ –ª–æ–≥–∏"""
        self.cursor.execute('''
            SELECT * FROM logs 
            ORDER BY created_at DESC 
            LIMIT ?
        ''', (limit,))
        return [dict(row) for row in self.cursor.fetchall()]

    # ========== –ü–û–õ–¨–ó–û–í–ê–¢–ï–õ–ò ==========

    def user_exists(self, user_id: int) -> bool:
        """–ü—Ä–æ–≤–µ—Ä—è–µ—Ç —Å—É—â–µ—Å—Ç–≤–æ–≤–∞–Ω–∏–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è"""
        self.cursor.execute('SELECT 1 FROM users WHERE user_id = ?', (user_id,))
        return bool(self.cursor.fetchone())

    def add_user(self, user_id: int, username: str = None):
        """–î–æ–±–∞–≤–ª—è–µ—Ç –Ω–æ–≤–æ–≥–æ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è"""
        try:
            self.cursor.execute('''
                INSERT OR IGNORE INTO users (user_id, username)
                VALUES (?, ?)
            ''', (user_id, username))
            self.conn.commit()
        except Exception as e:
            logger.error(f"–û—à–∏–±–∫–∞ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è: {e}")

    def get_user(self, user_id: int) -> Optional[Dict]:
        """–ü–æ–ª—É—á–∞–µ—Ç –¥–∞–Ω–Ω—ã–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è"""
        self.cursor.execute('SELECT * FROM users WHERE user_id = ?', (user_id,))
        row = self.cursor.fetchone()
        return dict(row) if row else None

    def get_user_role(self, user_id: int) -> int:
        """–ü–æ–ª—É—á–∞–µ—Ç —Ä–æ–ª—å –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è"""
        user = self.get_user(user_id)
        return user['role_id'] if user else 0

    def update_role(self, user_id: int, role_id: int, granted_by_id: int = None):
        """–û–±–Ω–æ–≤–ª—è–µ—Ç —Ä–æ–ª—å –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è"""
        try:
            self.cursor.execute('''
                UPDATE users 
                SET role_id = ?, granted_by_id = ?
                WHERE user_id = ?
            ''', (role_id, granted_by_id, user_id))
            self.conn.commit()
            logger.info(f"–†–æ–ª—å –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è {user_id} –æ–±–Ω–æ–≤–ª–µ–Ω–∞ –Ω–∞ {role_id}")
            
            # –õ–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ –≤—ã–¥–∞—á–∏ —Ä–æ–ª–∏
            if granted_by_id:
                self.add_log(
                    action_type="ROLE_GRANT",
                    admin_id=granted_by_id,
                    target_id=user_id,
                    details=f"–†–æ–ª—å –∏–∑–º–µ–Ω–µ–Ω–∞ –Ω–∞ {ROLES[role_id]['name']}"
                )
        except Exception as e:
            logger.error(f"–û—à–∏–±–∫–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Ä–æ–ª–∏: {e}")

    # ========== –ü–†–ï–ú–ò–£–ú ==========

    def is_premium_user(self, user_id: int) -> bool:
        """–ü—Ä–æ–≤–µ—Ä—è–µ—Ç –Ω–∞–ª–∏—á–∏–µ –∞–∫—Ç–∏–≤–Ω–æ–≥–æ –ø—Ä–µ–º–∏—É–º–∞"""
        try:
            self.cursor.execute('''
                SELECT premium_expiry FROM premium_users 
                WHERE user_id = ? AND premium_expiry > datetime('now')
            ''', (user_id,))
            row = self.cursor.fetchone()
            return bool(row)
        except Exception as e:
            logger.error(f"–û—à–∏–±–∫–∞ –ø—Ä–æ–≤–µ—Ä–∫–∏ –ø—Ä–µ–º–∏—É–º–∞: {e}")
            return False

    def get_premium_expiry(self, user_id: int) -> Optional[str]:
        """–ü–æ–ª—É—á–∞–µ—Ç –¥–∞—Ç—É –æ–∫–æ–Ω—á–∞–Ω–∏—è –ø—Ä–µ–º–∏—É–º–∞"""
        try:
            self.cursor.execute('SELECT premium_expiry FROM premium_users WHERE user_id = ?', (user_id,))
            row = self.cursor.fetchone()
            return row['premium_expiry'] if row else None
        except:
            return None

    def add_premium(self, user_id: int, expiry_date: str, granted_by_id: int = None):
        """–î–æ–±–∞–≤–ª—è–µ—Ç/–æ–±–Ω–æ–≤–ª—è–µ—Ç –ø—Ä–µ–º–∏—É–º —Å—Ç–∞—Ç—É—Å"""
        try:
            self.cursor.execute('''
                INSERT OR REPLACE INTO premium_users (user_id, premium_expiry, granted_by, granted_at)
                VALUES (?, ?, ?, ?)
            ''', (user_id, expiry_date, granted_by_id, datetime.now().isoformat()))
            self.conn.commit()
            logger.info(f"–ü—Ä–µ–º–∏—É–º –¥–ª—è {user_id} —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω –¥–æ {expiry_date}")
            
            # –õ–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ –≤—ã–¥–∞—á–∏ –ø—Ä–µ–º–∏—É–º–∞
            if granted_by_id:
                self.add_log(
                    action_type="PREMIUM_GRANT",
                    admin_id=granted_by_id,
                    target_id=user_id,
                    details=f"–ü—Ä–µ–º–∏—É–º –¥–æ {expiry_date}"
                )
        except Exception as e:
            logger.error(f"–û—à–∏–±–∫–∞ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –ø—Ä–µ–º–∏—É–º–∞: {e}")

    def remove_premium(self, user_id: int):
        """–£–¥–∞–ª—è–µ—Ç –ø—Ä–µ–º–∏—É–º —Å—Ç–∞—Ç—É—Å"""
        try:
            self.cursor.execute('DELETE FROM premium_users WHERE user_id = ?', (user_id,))
            self.conn.commit()
            logger.info(f"–ü—Ä–µ–º–∏—É–º —É–¥–∞–ª–µ–Ω –¥–ª—è {user_id}")
        except Exception as e:
            logger.error(f"–û—à–∏–±–∫–∞ —É–¥–∞–ª–µ–Ω–∏—è –ø—Ä–µ–º–∏—É–º–∞: {e}")

    def set_custom_photo(self, user_id: int, photo_url: str):
        """–£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ—Ç –∫–∞—Å—Ç–æ–º–Ω–æ–µ —Ñ–æ—Ç–æ –¥–ª—è –ø—Ä–µ–º–∏—É–º –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è"""
        try:
            if self.is_premium_user(user_id):
                self.cursor.execute('''
                    UPDATE premium_users 
                    SET custom_photo_url = ?
                    WHERE user_id = ?
                ''', (photo_url, user_id))
                self.conn.commit()
                logger.info(f"–ö–∞—Å—Ç–æ–º–Ω–æ–µ —Ñ–æ—Ç–æ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ –¥–ª—è {user_id}")
                return True
            else:
                return False
        except Exception as e:
            logger.error(f"–û—à–∏–±–∫–∞ —É—Å—Ç–∞–Ω–æ–≤–∫–∏ –∫–∞—Å—Ç–æ–º–Ω–æ–≥–æ —Ñ–æ—Ç–æ: {e}")
            return False

    def get_custom_photo(self, user_id: int) -> Optional[str]:
        """–ü–æ–ª—É—á–∞–µ—Ç –∫–∞—Å—Ç–æ–º–Ω–æ–µ —Ñ–æ—Ç–æ –ø—Ä–µ–º–∏—É–º –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è"""
        try:
            if self.is_premium_user(user_id):
                self.cursor.execute('SELECT custom_photo_url FROM premium_users WHERE user_id = ?', (user_id,))
                row = self.cursor.fetchone()
                return row['custom_photo_url'] if row else None
            else:
                return None
        except:
            return None

    def set_custom_channel(self, user_id: int, channel: str):
