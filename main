        volunteers.extend([row['user_id'] for row in rows])
    
    if not volunteers:
        await loading_message.edit("–ù–∞ –¥–∞–Ω–Ω—ã–π –º–æ–º–µ–Ω—Ç –≤–æ–ª–æ–Ω—Ç—ë—Ä–æ–≤ –Ω–µ—Ç ‚õî")
        return
    
