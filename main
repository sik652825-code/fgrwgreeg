@bot.on(events.CallbackQuery(pattern=r'buy_(premium|rest)_(\d+)([–¥–Ω–º])'))
async def purchase_handler(event):
    """–ü–æ–∫—É–ø–∫–∞ –≤ –º–∞–≥–∞–∑–∏–Ω–µ"""
    try:
        data = event.data.decode('utf-8')
        parts = data.split('_')
        
        if len(parts) < 3:
            return await event.answer("‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π —Ñ–æ—Ä–º–∞—Ç –¥–∞–Ω–Ω—ã—Ö", alert=True)
            
        action = parts[1]  # premium –∏–ª–∏ rest
        if len(parts) >= 3:
            amount = int(parts[2][:-1])  # —É–¥–∞–ª—è–µ–º –ø–æ—Å–ª–µ–¥–Ω–∏–π —Å–∏–º–≤–æ–ª (–¥, –Ω, –º)
            unit = parts[2][-1]  # –ø–æ—Å–ª–µ–¥–Ω–∏–π —Å–∏–º–≤–æ–ª (–¥, –Ω, –º)
        else:
            return await event.answer("‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π —Ñ–æ—Ä–º–∞—Ç –¥–∞–Ω–Ω—ã—Ö", alert=True)
    
        user_id = event.sender_id
        balance = db.get_premium_points(user_id)
        
        cost_mapping = {
            'premium_1–¥': 10,
            'premium_7–¥': 50,
            'premium_30–¥': 125,
            'rest_1–¥': 100
        }
        
        cost = cost_mapping.get(data)
        
        if not cost:
            return await event.answer("–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–∞—è –∫–æ–º–∞–Ω–¥–∞.", alert=True)
        
        if balance >= cost:
            db.add_premium_points(user_id, -cost)
            
            if action == 'premium':
                if unit == '–¥':
                    days = amount
                elif unit == '–Ω':
                    days = amount * 7
                elif unit == '–º':
                    days = amount * 30
                else:
                    return await event.answer("‚ùå –ù–µ–≤–µ—Ä–Ω–∞—è –µ–¥–∏–Ω–∏—Ü–∞ –≤—Ä–µ–º–µ–Ω–∏", alert=True)
                
                expiry_date = (datetime.now() + timedelta(days=days)).strftime("%Y-%m-%d %H:%M:%S")
                db.add_premium(user_id, expiry_date)
                await event.answer(f"‚úÖ –ü—Ä–µ–º–∏—É–º –Ω–∞ {amount}{unit} —É—Å–ø–µ—à–Ω–æ –ø—Ä–∏–æ–±—Ä–µ—Ç–µ–Ω!", alert=True)
            elif action == 'rest':
                await event.answer("‚úÖ –û—Ç–¥—ã—Ö —É—Å–ø–µ—à–Ω–æ –ø—Ä–∏–æ–±—Ä–µ—Ç–µ–Ω!", alert=True)
            else:
                await event.answer("‚ùå –ù–µ–∏–∑–≤–µ—Å—Ç–Ω—ã–π —Ç–∏–ø —Ç–æ–≤–∞—Ä–∞", alert=True)
        else:
            await event.answer("‚ùå –£ –≤–∞—Å –Ω–µ–¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ –∫–æ–∏–Ω–æ–≤!", alert=True)
    except Exception as e:
        logger.error(f"–û—à–∏–±–∫–∞ –≤ purchase_handler: {e}")
        await event.answer("‚ùå –ü—Ä–æ–∏–∑–æ—à–ª–∞ –æ—à–∏–±–∫–∞ –ø—Ä–∏ –ø–æ–∫—É–ø–∫–µ", alert=True)

