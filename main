            "‚ùå –£ –≤–∞—Å –Ω–µ—Ç –ø—Ä–µ–º–∏—É–º —Å—Ç–∞—Ç—É—Å–∞! –î–ª—è —É—Å—Ç–∞–Ω–æ–≤–∫–∏ –∫–∞—Å—Ç–æ–º–Ω–æ–≥–æ —Ñ–æ—Ç–æ –ø—Ä–∏–æ–±—Ä–µ—Ç–∏—Ç–µ –ø—Ä–µ–º–∏—É–º.",
            alert=True
        )
    
    await event.answer("üñºÔ∏è –û—Ç–ø—Ä–∞–≤—å—Ç–µ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ –∏–ª–∏ –≤–∏–¥–µ–æ –¥–ª—è —É—Å—Ç–∞–Ω–æ–≤–∫–∏ –∫–∞—Å—Ç–æ–º–Ω–æ–≥–æ —Ñ–æ—Ç–æ")
    
    # –°–æ—Ö—Ä–∞–Ω—è–µ–º —Å–æ—Å—Ç–æ—è–Ω–∏–µ –¥–ª—è –æ–∂–∏–¥–∞–Ω–∏—è –º–µ–¥–∏–∞
    TEMP_STORAGE[f"awaiting_photo_{user_id}"] = True
    
    @bot.on(events.NewMessage(from_users=user_id))
    async def photo_input_handler(photo_event):
        if f"awaiting_photo_{user_id}" not in TEMP_STORAGE:
            return
        
        if photo_event.photo or photo_event.video:
            try:
                if photo_event.photo:
                    media_path = await bot.download_media(photo_event.photo)
                    
                    with open(media_path, "rb") as image_file:
                        files = {"image": image_file}
                        params = {"key": IMG_API_KEY}
                        response = requests.post(
                            "https://api.imgbb.com/1/upload",
                            params=params,
                            files=files
                        )
                        response.raise_for_status()
                        data = response.json()
                    
                    os.remove(media_path)
                    
                    if data.get("success") and "data" in data and "url" in data["data"]:
                        image_url = data["data"]["url"]
                        db.set_custom_photo(user_id, image_url)
                        await photo_event.reply(f"‚úÖ –ö–∞—Å—Ç–æ–º–Ω–æ–µ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ —É—Å–ø–µ—à–Ω–æ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ!")
                    else:
                        await photo_event.reply("‚ùå –ù–µ —É–¥–∞–ª–æ—Å—å –ø–æ–ª—É—á–∏—Ç—å URL –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è.")
                
                elif photo_event.video:
                    video_url = f"https://t.me/PlayingANTISCAM_bot?start=video_{photo_event.video.id}"
                    db.set_custom_photo(user_id, video_url)
                    await photo_event.reply(f"‚úÖ –ö–∞—Å—Ç–æ–º–Ω–æ–µ –≤–∏–¥–µ–æ —É—Å–ø–µ—à–Ω–æ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ!")
            
            except Exception as e:
                await photo_event.reply(f"‚ùå –ü—Ä–æ–∏–∑–æ—à–ª–∞ –æ—à–∏–±–∫–∞: {str(e)}")
        else:
            await photo_event.reply("‚ùå –ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –æ—Ç–ø—Ä–∞–≤—å—Ç–µ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ –∏–ª–∏ –≤–∏–¥–µ–æ.")
        
        # –£–¥–∞–ª—è–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫
        if f"awaiting_photo_{user_id}" in TEMP_STORAGE:
            del TEMP_STORAGE[f"awaiting_photo_{user_id}"]
            bot.remove_event_handler(photo_input_handler)
    
    # –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –≤—Ä–µ–º—è –æ–∂–∏–¥–∞–Ω–∏—è
    async def timeout_handler():
        await asyncio.sleep(60)
        if f"awaiting_photo_{user_id}" in TEMP_STORAGE:
            del TEMP_STORAGE[f"awaiting_photo_{user_id}"]
    
    asyncio.create_task(timeout_handler())

