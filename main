    # ========== –°–ö–ê–ú–ú–ï–†–´ ==========

    def add_scammer(self, user_id: int, reason: str, added_by: int, evidence: str, unique_id: str):
        """–î–æ–±–∞–≤–ª—è–µ—Ç —Å–∫–∞–º–º–µ—Ä–∞"""
        try:
            if not self.user_exists(user_id):
                self.add_user(user_id)
            
            # –ü–æ–ª—É—á–∞–µ–º username –¥–ª—è –ª–æ–≥–∏—Ä–æ–≤–∞–Ω–∏—è
            added_by_user = self.get_user(added_by)
            scammer_user = self.get_user(user_id)
            
            added_by_username = added_by_user.get('username', str(added_by)) if added_by_user else str(added_by)
            scammer_username = scammer_user.get('username', str(user_id)) if scammer_user else str(user_id)
            
            # –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ä–æ–ª—å —Å–∫–∞–º–º–µ—Ä–∞
            self.update_role(user_id, 3, added_by)
            
            # –î–æ–±–∞–≤–ª—è–µ–º –≤ —Ç–∞–±–ª–∏—Ü—É —Å–∫–∞–º–º–µ—Ä–æ–≤
            self.cursor.execute('''
                INSERT OR REPLACE INTO scammers 
                (user_id, reason, added_by, evidence, unique_id, added_by_username, scammer_username, granted_by_id)
                VALUES (?, ?, ?, ?, ?, ?, ?, ?)
            ''', (user_id, reason, added_by, evidence, unique_id, added_by_username, scammer_username, added_by))
            
            # –î–æ–±–∞–≤–ª—è–µ–º –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ –ø—Ä–∏—á–∏–Ω—ã
            self.add_additional_reason(user_id, reason)
            
            self.conn.commit()
            logger.info(f"–°–∫–∞–º–º–µ—Ä {user_id} –¥–æ–±–∞–≤–ª–µ–Ω. –ü—Ä–∏—á–∏–Ω–∞: {reason}")
            
            # –õ–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è —Å–∫–∞–º–º–µ—Ä–∞
            self.add_log(
                action_type="SCAMMER_ADDED",
                admin_id=added_by,
                target_id=user_id,
                details=f"–ü—Ä–∏—á–∏–Ω–∞: {reason}"
            )
            
            return True
        except Exception as e:
            logger.error(f"–û—à–∏–±–∫–∞ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è —Å–∫–∞–º–º–µ—Ä–∞: {e}")
            return False

    def remove_scammer_status(self, user_id: int, removed_by: int = None):
        """–£–¥–∞–ª—è–µ—Ç —Å—Ç–∞—Ç—É—Å —Å–∫–∞–º–º–µ—Ä–∞"""
        try:
            self.cursor.execute('DELETE FROM scammers WHERE user_id = ?', (user_id,))
            self.cursor.execute('DELETE FROM additional_reasons WHERE user_id = ?', (user_id,))
            self.update_role(user_id, 0, removed_by)
            self.conn.commit()
            logger.info(f"–°—Ç–∞—Ç—É—Å —Å–∫–∞–º–º–µ—Ä–∞ —Å–Ω—è—Ç —Å {user_id}")
            
            # –õ–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ —Å–Ω—è—Ç–∏—è —Å—Ç–∞—Ç—É—Å–∞ —Å–∫–∞–º–º–µ—Ä–∞
            if removed_by:
                self.add_log(
                    action_type="SCAMMER_REMOVED",
                    admin_id=removed_by,
                    target_id=user_id,
                    details="–°—Ç–∞—Ç—É—Å —Å–∫–∞–º–º–µ—Ä–∞ —Å–Ω—è—Ç"
                )
        except Exception as e:
            logger.error(f"–û—à–∏–±–∫–∞ —É–¥–∞–ª–µ–Ω–∏—è —Å—Ç–∞—Ç—É—Å–∞ —Å–∫–∞–º–º–µ—Ä–∞: {e}")

    def get_scammer_info(self, user_id: int) -> Optional[Dict]:
        """–ü–æ–ª—É—á–∞–µ—Ç –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ —Å–∫–∞–º–º–µ—Ä–µ"""
        self.cursor.execute('SELECT * FROM scammers WHERE user_id = ? ORDER BY added_at DESC LIMIT 1', (user_id,))
        row = self.cursor.fetchone()
        return dict(row) if row else None

    def get_additional_reasons(self, user_id: int) -> List[str]:
        """–ü–æ–ª—É—á–∞–µ—Ç –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ –ø—Ä–∏—á–∏–Ω—ã –¥–ª—è —Å–∫–∞–º–º–µ—Ä–∞"""
        self.cursor.execute('SELECT additional_reason FROM additional_reasons WHERE user_id = ?', (user_id,))
        rows = self.cursor.fetchall()
        return [row['additional_reason'] for row in rows] if rows else []

    def add_additional_reason(self, user_id: int, additional_reason: str):
        """–î–æ–±–∞–≤–ª—è–µ—Ç –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—É—é –ø—Ä–∏—á–∏–Ω—É"""
        try:
            self.cursor.execute('''
                INSERT INTO additional_reasons (user_id, additional_reason)
                VALUES (?, ?)
            ''', (user_id, additional_reason))
            self.conn.commit()
            logger.info(f"–î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –ø—Ä–∏—á–∏–Ω–∞ –¥–æ–±–∞–≤–ª–µ–Ω–∞ –¥–ª—è {user_id}")
            return True
        except Exception as e:
            logger.error(f"–û—à–∏–±–∫–∞ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–æ–π –ø—Ä–∏—á–∏–Ω—ã: {e}")
            return False

