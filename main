# ========== –ö–û–ú–ê–ù–î–ê +–ü–†–ï–ú–ò–£–ú ==========

@bot.on(events.NewMessage(pattern=r'(?i)^\+–ø—Ä–µ–º–∏—É–º\s+(\d+)([–¥–Ω–º])'))
async def add_premium_command(event):
    """–î–æ–±–∞–≤–ª–µ–Ω–∏–µ –ø—Ä–µ–º–∏—É–º —Å—Ç–∞—Ç—É—Å–∞ —Å –ª–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ–º"""
    user_id = event.sender_id
    
    # –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–∞–≤
    if not can_manage_roles(user_id, 10):  # 10 - —Å–æ–∑–¥–∞—Ç–µ–ª—å, –º–æ–∂–µ—Ç –≤—ã–¥–∞–≤–∞—Ç—å –ø—Ä–µ–º–∏—É–º
        return await send_response(event, "‚ùå –£ –≤–∞—Å –Ω–µ—Ç –ø—Ä–∞–≤ –¥–ª—è –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è —ç—Ç–æ–π –∫–æ–º–∞–Ω–¥—ã.")
    
    if not event.is_reply:
        return await send_response(event, "‚ùå –û—Ç–≤–µ—Ç—å—Ç–µ –Ω–∞ —Å–æ–æ–±—â–µ–Ω–∏–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è, –∫–æ—Ç–æ—Ä–æ–º—É –Ω—É–∂–Ω–æ –≤—ã–¥–∞—Ç—å –ø—Ä–µ–º–∏—É–º.")
    
    replied = await event.get_reply_message()
    target_user = await get_entity_safe(replied.sender_id)
    
    text = event.raw_text
    match = re.search(r'\+–ø—Ä–µ–º–∏—É–º\s+(\d+)([–¥–Ω–º])', text, re.IGNORECASE)
    if not match:
        return await send_response(event, "‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π —Ñ–æ—Ä–º–∞—Ç. –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ: +–ø—Ä–µ–º–∏—É–º 30–¥ (30 –¥–Ω–µ–π)")
    
    amount = int(match.group(1))
    unit = match.group(2).lower()
    
    # –ö–æ–Ω–≤–µ—Ä—Ç–∏—Ä—É–µ–º –≤ –¥–Ω–∏
    if unit == '–¥':
        days = amount
        time_text = f"{amount} –¥–Ω–µ–π"
    elif unit == '–Ω':
        days = amount * 7
        time_text = f"{amount} –Ω–µ–¥–µ–ª—å"
    elif unit == '–º':
        days = amount * 30
        time_text = f"{amount} –º–µ—Å—è—Ü–µ–≤"
    else:
        return await send_response(event, "‚ùå –ù–µ–≤–µ—Ä–Ω–∞—è –µ–¥–∏–Ω–∏—Ü–∞ –≤—Ä–µ–º–µ–Ω–∏. –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ: –¥ (–¥–Ω–∏), –Ω (–Ω–µ–¥–µ–ª–∏), –º (–º–µ—Å—è—Ü—ã)")
    
    # –î–æ–±–∞–≤–ª—è–µ–º –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è –≤ –ë–î –ø–µ—Ä–µ–¥ –≤—ã–¥–∞—á–µ–π –ø—Ä–µ–º–∏—É–º–∞
    if not db.user_exists(target_user.id):
        db.add_user(target_user.id, target_user.username)
        logger.info(f"–ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å {target_user.id} –¥–æ–±–∞–≤–ª–µ–Ω –≤ –ë–î –ø–µ—Ä–µ–¥ –≤—ã–¥–∞—á–µ–π –ø—Ä–µ–º–∏—É–º–∞")
    
    # –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –¥–∞—Ç—É –æ–∫–æ–Ω—á–∞–Ω–∏—è –ø—Ä–µ–º–∏—É–º–∞
    expiry_date = (datetime.now() + timedelta(days=days)).strftime("%Y-%m-%d %H:%M:%S")
    db.add_premium(target_user.id, expiry_date, event.sender_id)
    
    # –õ–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ –≤—ã–¥–∞—á–∏ –ø—Ä–µ–º–∏—É–º–∞
    db.add_log(
        action_type="PREMIUM_GRANT",
        admin_id=event.sender_id,
        target_id=target_user.id,
        details=f"–ü—Ä–µ–º–∏—É–º –¥–æ {expiry_date}"
    )
    
    await send_response(
        event,
        f"‚úÖ –ü—Ä–µ–º–∏—É–º —Å—Ç–∞—Ç—É—Å –Ω–∞ {time_text} —É—Å–ø–µ—à–Ω–æ –≤—ã–¥–∞–Ω –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—é "
        f"[{target_user.first_name}](tg://user/{target_user.id})"
    )

# ========== –ö–û–ú–ê–ù–î–ê -–ü–†–ï–ú–ò–£–ú ==========

@bot.on(events.NewMessage(pattern=r'(?i)^\-–ø—Ä–µ–º–∏—É–º'))
async def remove_premium_command(event):
    """–£–¥–∞–ª–µ–Ω–∏–µ –ø—Ä–µ–º–∏—É–º —Å—Ç–∞—Ç—É—Å–∞"""
    user_id = event.sender_id
    
    # –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–∞–≤
    if not can_manage_roles(user_id, 10):
        return await send_response(event, "‚ùå –£ –≤–∞—Å –Ω–µ—Ç –ø—Ä–∞–≤ –¥–ª—è –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è —ç—Ç–æ–π –∫–æ–º–∞–Ω–¥—ã.")
    
    if not event.is_reply:
        return await send_response(event, "‚ùå –û—Ç–≤–µ—Ç—å—Ç–µ –Ω–∞ —Å–æ–æ–±—â–µ–Ω–∏–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è, —É –∫–æ—Ç–æ—Ä–æ–≥–æ –Ω—É–∂–Ω–æ —É–¥–∞–ª–∏—Ç—å –ø—Ä–µ–º–∏—É–º.")
    
    replied = await event.get_reply_message()
    target_user = await get_entity_safe(replied.sender_id)
    
    db.remove_premium(target_user.id)
    
    # –õ–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ —É–¥–∞–ª–µ–Ω–∏—è –ø—Ä–µ–º–∏—É–º–∞
    db.add_log(
        action_type="PREMIUM_REMOVED",
        admin_id=event.sender_id,
        target_id=target_user.id,
        details="–ü—Ä–µ–º–∏—É–º —É–¥–∞–ª–µ–Ω"
    )
    
    await send_response(
        event,
        f"‚úÖ –ü—Ä–µ–º–∏—É–º —Å—Ç–∞—Ç—É—Å —É–¥–∞–ª–µ–Ω —É –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è "
        f"[{target_user.first_name}](tg://user/{target_user.id})"
    )

