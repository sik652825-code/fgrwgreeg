@bot.on(events.CallbackQuery(pattern=r'set_country_(\d+)'))
async def set_country_handler(event):
    """–£—Å—Ç–∞–Ω–æ–≤–∫–∞ —Å—Ç—Ä–∞–Ω—ã"""
    try:
        data = event.data.decode('utf-8')
        country_idx = int(data.split('_')[2])
        countries = [
            "–°–®–ê üá∫üá∏", "–ö–∞–Ω–∞–¥–∞ üá®üá¶", "–ú–µ–∫—Å–∏–∫–∞ üá≤üáΩ", "–ë—Ä–∞–∑–∏–ª–∏—è üáßüá∑",
            "–ê—Ä–≥–µ–Ω—Ç–∏–Ω–∞ üá¶üá∑", "–í–µ–ª–∏–∫–æ–±—Ä–∏—Ç–∞–Ω–∏—è üá¨üáß", "–§—Ä–∞–Ω—Ü–∏—è üá´üá∑",
            "–ì–µ—Ä–º–∞–Ω–∏—è üá©üá™", "–ò—Ç–∞–ª–∏—è üáÆüáπ", "–ò—Å–ø–∞–Ω–∏—è üá™üá∏", "–ö–∏—Ç–∞–π üá®üá≥",
            "–Ø–ø–æ–Ω–∏—è üáØüáµ", "–ê–≤—Å—Ç—Ä–∞–ª–∏—è üá¶üá∫", "–ò–Ω–¥–∏—è üáÆüá≥", "–†–æ—Å—Å–∏—è üá∑üá∫"
        ]
        
        country = countries[country_idx]
        user_id = event.sender_id
        
        # –î–æ–±–∞–≤–ª—è–µ–º –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è –≤ –ë–î –µ—Å–ª–∏ –µ–≥–æ –Ω–µ—Ç
        if not db.user_exists(user_id):
            db.add_user(user_id, None)
        
        db.update_user(user_id, country=country)
        await event.answer(f"‚úÖ –°—Ç—Ä–∞–Ω–∞ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∞: {country}", alert=True)
    except Exception as e:
        logger.error(f"–û—à–∏–±–∫–∞ –≤ set_country_handler: {e}")
        await event.answer("‚ùå –û—à–∏–±–∫–∞ —É—Å—Ç–∞–Ω–æ–≤–∫–∏ —Å—Ç—Ä–∞–Ω—ã", alert=True)

# ========== –ò–°–ü–†–ê–í–õ–ï–ù–ù–´–ï –ö–û–õ–ë–≠–ö–ò –î–õ–Ø –ö–ê–ù–ê–õ–ê –ò –§–û–¢–û ==========

@bot.on(events.CallbackQuery(pattern='channel_soon'))
async def channel_soon_handler_fixed(event):
    """–£—Å—Ç–∞–Ω–æ–≤–∫–∞ –∫–∞–Ω–∞–ª–∞ - –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–Ω–∞—è –≤–µ—Ä—Å–∏—è"""
    user_id = event.sender_id
    
    # –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–µ–º–∏—É–º-—Å—Ç–∞—Ç—É—Å–∞
    if not db.is_premium_user(user_id):
        return await event.answer(
            "‚ùå –£ –≤–∞—Å –Ω–µ—Ç –ø—Ä–µ–º–∏—É–º —Å—Ç–∞—Ç—É—Å–∞! –î–ª—è —É—Å—Ç–∞–Ω–æ–≤–∫–∏ –∫–∞–Ω–∞–ª–∞ –ø—Ä–∏–æ–±—Ä–µ—Ç–∏—Ç–µ –ø—Ä–µ–º–∏—É–º.",
            alert=True
        )
    
    await event.answer("üì¢ –í–≤–µ–¥–∏—Ç–µ username –∫–∞–Ω–∞–ª–∞ (–Ω–∞–ø—Ä–∏–º–µ—Ä, @channelname)")
    
    # –°–æ—Ö—Ä–∞–Ω—è–µ–º —Å–æ—Å—Ç–æ—è–Ω–∏–µ –¥–ª—è –æ–∂–∏–¥–∞–Ω–∏—è –≤–≤–æ–¥–∞ –∫–∞–Ω–∞–ª–∞
    TEMP_STORAGE[f"awaiting_channel_{user_id}"] = True
    
    @bot.on(events.NewMessage(from_users=user_id))
    async def channel_input_handler(channel_event):
        if f"awaiting_channel_{user_id}" not in TEMP_STORAGE:
            return
        
        channel_name = channel_event.text.strip()
        
        # –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ñ–æ—Ä–º–∞—Ç –∫–∞–Ω–∞–ª–∞
        if not channel_name.startswith('@'):
            await channel_event.reply("‚ùå –ò–º—è –∫–∞–Ω–∞–ª–∞ –¥–æ–ª–∂–Ω–æ –Ω–∞—á–∏–Ω–∞—Ç—å—Å—è —Å @ (–Ω–∞–ø—Ä–∏–º–µ—Ä, @channelname)")
            return
        
        if len(channel_name) > 32:
            await channel_event.reply("‚ùå –ò–º—è –∫–∞–Ω–∞–ª–∞ —Å–ª–∏—à–∫–æ–º –¥–ª–∏–Ω–Ω–æ–µ (–º–∞–∫—Å. 32 —Å–∏–º–≤–æ–ª–∞)")
            return
        
        # –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∫–∞–Ω–∞–ª
        db.set_custom_channel(user_id, channel_name)
        
        await channel_event.reply(f"‚úÖ –ö–∞–Ω–∞–ª {channel_name} —É—Å–ø–µ—à–Ω–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω!")
        
        # –£–¥–∞–ª—è–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫
        del TEMP_STORAGE[f"awaiting_channel_{user_id}"]
        bot.remove_event_handler(channel_input_handler)
    
    # –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –≤—Ä–µ–º—è –æ–∂–∏–¥–∞–Ω–∏—è
    async def timeout_handler():
        await asyncio.sleep(60)
        if f"awaiting_channel_{user_id}" in TEMP_STORAGE:
            del TEMP_STORAGE[f"awaiting_channel_{user_id}"]
    
    asyncio.create_task(timeout_handler())

